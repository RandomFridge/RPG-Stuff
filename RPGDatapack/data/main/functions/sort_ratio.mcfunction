#体力が50%ならA～Kのアルファベットをstorageに代入
    execute if score @s hp_ratio matches 50..54 run data modify storage health: buffer set value "A"
    execute if score @s hp_ratio matches 55..59 run data modify storage health: buffer set value "B"
    execute if score @s hp_ratio matches 60..64 run data modify storage health: buffer set value "C"
    execute if score @s hp_ratio matches 65..69 run data modify storage health: buffer set value "D"
    execute if score @s hp_ratio matches 70..74 run data modify storage health: buffer set value "E"
    execute if score @s hp_ratio matches 75..79 run data modify storage health: buffer set value "F"
    execute if score @s hp_ratio matches 80..84 run data modify storage health: buffer set value "G"
    execute if score @s hp_ratio matches 85..89 run data modify storage health: buffer set value "H"
    execute if score @s hp_ratio matches 90..95 run data modify storage health: buffer set value "I"
    execute if score @s hp_ratio matches 96..99 run data modify storage health: buffer set value "J"
    execute if score @s hp_ratio matches 100 run data modify storage health: buffer set value "K"
